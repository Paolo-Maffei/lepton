# >
# Support for GDB stubs
# The HAL implements GDB stubs for the target.
#
cdl_interface CYGINT_HAL_DEBUG_GDB_STUBS {
    # Implemented by CYGPKG_HAL_ARM_AT91, active, enabled
    # This value cannot be modified here.
    # Flavor: data
    # Current_value: 1

